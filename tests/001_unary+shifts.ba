#{
	Expected output:
	17 21 17
	12345678902357111317
	-21 12
	18446744073709551604 12 12
	1 0 0
	-1 0 11
	-1 13
	12 12 64 2
	2 64 2 2
#}

f"%i{2 + 5 * 3} %i{(2 + 5) * 3} %i{2 + (5 * 3)}\n";

i64 x = 12345678902357111317;
i64 y = -21;
f"%i{x}\n%i{y} ";
u64 z = 5 + 10 * 15 // 20;
u64 a = -z;
f"%u{z}\n%u{a} %u{-a} %u{- -z}\n";

i64 zero = 0;
i64 one = !zero;
f"%i{!zero} %i{!!zero} %i{!one}\n"
f"%i{-!zero} %i{!!zero} %i{~a}\n";
f"%i{~zero} %i{+-~z}\n";

f"%i{6 << one} %i{6 << - -one} %i{one << 6} %i{one << one}\n";
f"%i{one << - -one} %i{- -one << 6} %i{- -one << one} %i{- -one << - -one}\n";

