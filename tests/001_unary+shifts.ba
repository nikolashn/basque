write 2 + 5 * 3; " ";
write (2 + 5) * 3; " ";
write 2 + (5 * 3); "\n";

i64 x = 12345678902357111317;
i64 y = -21;
write x; "\n";
write y; " ";
u64 z = 5 + 10 * 15 // 20;
u64 a = -z;
write z; "\n";
write a; " ";
write -a; " ";
write - -z; "\n";

i64 zero = 0;
i64 one = !zero;
write !zero; " ";
write !!zero; " ";
write !one; "\n";
write -!zero; " ";
write !!zero; " ";
write ~a; "\n";
write ~zero; " ";
write +-~z; "\n";

write 6 << one; " ";
write 6 << - -one; " ";
write one << 6; " ";
write one << one; "\n";
write one << - -one; " ";
write - -one << 6; " ";
write - -one << one; " ";
write - -one << - -one; "\n";
