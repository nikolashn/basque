void Hello() {
	write "Hello world!\n";
}

Hello(); # Hello world!
Hello(); # Hello world!

void WriteI64(i64 x = 0) {
	if x < 0 {
		write "-";
		x = -x;
	}
	write x;
}

WriteI64(); "\n"; # 0
WriteI64(5); "\n"; # 5
WriteI64(-21); "\n"; # -21

i64 n = -1 * 2 * -3 * 4 * -5;
WriteI64(n); "\n"; # -120

i64 Mul(i64 x, i64 y) {
	i64 a = x * y;
	return a;
}

# 9 * 10 = 90
"9 * 10 = ";
write Mul(9, 10); "\n";

# -120 * -120 * -120 = -1728000
WriteI64(n); " * ";
WriteI64(n); " * ";
WriteI64(n); " = ";
WriteI64(Mul(Mul(n,n), n)); "\n";

